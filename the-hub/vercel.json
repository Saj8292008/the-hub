{
  "buildCommand": "npm install && vite build && mv dist/index.html dist/index-app.html && cp public/landing.html dist/index.html",
  "installCommand": "npm install",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    { "source": "/app", "destination": "/index-app.html" },
    { "source": "/app/(.*)", "destination": "/index-app.html" },
    { "source": "/signup", "destination": "/index-app.html" },
    { "source": "/signup/(.*)", "destination": "/index-app.html" },
    { "source": "/login", "destination": "/index-app.html" },
    { "source": "/login/(.*)", "destination": "/index-app.html" },
    { "source": "/verify-email-address", "destination": "/index-app.html" },
    { "source": "/verify-phone-number", "destination": "/index-app.html" },
    { "source": "/sso-callback", "destination": "/index-app.html" },
    { "source": "/dashboard", "destination": "/index-app.html" },
    { "source": "/dashboard/(.*)", "destination": "/index-app.html" },
    { "source": "/blog/(.*)", "destination": "/blog/$1" },
    { "source": "/guides/(.*)", "destination": "/guides/$1" }
  ]
}
